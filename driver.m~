function [f,xr] = driver() 
  %Función que carga los datos de la función e invoca las funciones del proyecto.
  
  %---------------------- Carga Datos ---------------- %
  
  data = dlmread('data.csv',';');
  Xo = [-1, -2, 1, -1];  
  t = data(:,1);
  optimo = (4*exp(-4*t))-(4*exp(-5*t));
  
  plot(t, optimo)
  
 end